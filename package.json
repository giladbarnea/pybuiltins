{
	"name": "pythonlang",
	"version": "1.0.3",
	"description": "Python built-ins in Javascript.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"start": "npm run build:live",
		"build": "tsc -p .",
		"testWatch": "jest --watchAll --bail=10",
