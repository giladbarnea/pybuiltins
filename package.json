		"@types/chance": "latest",
		"@types/jest": "^24.0.13",
		"@types/node": "^12.0.4",
		"chance": "^1.0.18",
		"jest": "^24.8.0",
		"perf_hooks": "latest",
		"ts-jest": "^24.0.2",
